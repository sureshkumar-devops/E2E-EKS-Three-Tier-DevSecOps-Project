pipeline
{
    agent any
    triggers { pollSCM '* * * * *' }
    tools
    {
        jdk 'JAVA_HOME'
    }
    environment
    {
        scanner_home= tool 'sonarqube-scanner'
    }
    stages
    {
        stage('Clean Workspace')
        {
            steps
            {
                cleanWs()
            }
        }
        stage('Git Checkout')
        {
            steps
            {
                git branch:'master', url:'https://github.com/sureshkumar-devops/E2E-EKS-Three-Tier-DevSecOps-Project.git'
            }
        }
       
    }
}